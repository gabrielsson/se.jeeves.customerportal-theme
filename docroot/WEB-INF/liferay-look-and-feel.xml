<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.2.5+</version>
	</compatibility>
	<theme id="se.jeeves.customerportal" name="Customer Portal Theme">
		<settings>
			<setting configurable="true" key="portlet-setup-show-borders-default" type="checkbox" value="true"></setting>
			<setting configurable="true" key="footer-article-id" value=""></setting>
		</settings>
		
		<color-scheme id="01" name="Normal">
			<default-cs>true</default-cs>
			<css-class>default</css-class>
		</color-scheme>
		<color-scheme id="02" name="SEAT">
			<css-class>seat</css-class>
		</color-scheme>
		<color-scheme id="03" name="Skoda">
			<css-class>skoda</css-class>
		</color-scheme>
		<color-scheme id="04" name="Volvo Trucks">
			<css-class>volvo-trucks</css-class>
		</color-scheme>
		<color-scheme id="05" name="Volvo Cars">
			<css-class>volvo-cars</css-class>
		</color-scheme>
		<color-scheme id="06" name="Dickson">
			<css-class>dickson</css-class>
		</color-scheme>
		<color-scheme id="07" name="Renault">
			<css-class>renault</css-class>
		</color-scheme>
	</theme>
</look-and-feel>